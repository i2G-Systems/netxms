<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            category="org.netxms.ui.eclipse.viewcategory.config"
            class="org.netxms.ui.eclipse.agentmanager.views.AgentConfigEditorView"
            icon="icons/agent_config.gif"
            id="org.netxms.ui.eclipse.agentmanager.views.AgentConfigEditorView"
            name="%view.AgentConfig"
            restorable="false">
      </view>
      <view
            allowMultiple="false"
            class="org.netxms.ui.eclipse.agentmanager.views.PackageManager"
            icon="icons/packages.png"
            id="org.netxms.ui.eclipse.agentmanager.views.PackageManager"
            name="%view.PackageManager"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            class="org.netxms.ui.eclipse.agentmanager.views.ServerStoredAgentConfigEditorView"
            icon="icons/config_manager.gif"
            id="org.netxms.ui.eclipse.agentmanager.views.ServerStoredAgentConfigEditorView"
            name="%view.AgentConfigManager"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            category="org.netxms.ui.eclipse.viewcategory.config"
            class="org.netxms.ui.eclipse.agentmanager.views.ScreenshotView"
            icon="icons/screenshot.png"
            id="org.netxms.ui.eclipse.agentmanager.views.ScreenshotView"
            name="%view.Screenshot"
            restorable="false">
      </view>
      <view
            allowMultiple="false"
            class="org.netxms.ui.eclipse.agentmanager.views.TunnelManager"
            icon="icons/tunnel_manager.png"
            id="org.netxms.ui.eclipse.agentmanager.views.TunnelManager"
            name="Agent Tunnels"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            class="org.netxms.ui.eclipse.agentmanager.views.UserAgentNotificationView"
            icon="icons/user_agent_messages.png"
            id="org.netxms.ui.eclipse.agentmanager.views.UserAgentNotificationView"
            name="User Support Application Notifications"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="org.netxms.ui.eclipse.agentmanager.actions.popup.object.Node"
            objectClass="org.netxms.client.objects.Node">
         <action
               class="org.netxms.ui.eclipse.agentmanager.actions.OpenAgentConfig"
               enablesFor="+"
               icon="icons/agent_config.gif"
               id="org.netxms.ui.eclipse.agentmanager.popupActions.OpenAgentConfig_Node"
               label="%menu.EditConfig"
               menubarPath="objectcontrol"
               style="push">
         </action>
         <action
               class="org.netxms.ui.eclipse.agentmanager.actions.TakeScreenshot"
               enablesFor="+"
               icon="icons/screenshot.png"
               id="org.netxms.ui.eclipse.agentmanager.popupActions.TakeScreenshot_Node"
               label="%menu.TakeScreenshot"
               menubarPath="objectcontrol"
               style="push">
         </action>
         <action
               class="org.netxms.ui.eclipse.agentmanager.actions.SendUserAgentNotification"
               enablesFor="+"
               icon="icons/user_agent_messages.png"
               id="org.netxms.ui.eclipse.agentmanager.popupActions.SendUserAgentNotification_Node"
               label="Send user support application notification"
               menubarPath="objectcontrol"
               style="push">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.netxms.ui.eclipse.agentmanager.actions.popup.object.Container"
            objectClass="org.netxms.client.objects.Container">
         <action
               class="org.netxms.ui.eclipse.agentmanager.actions.SendUserAgentNotification"
               enablesFor="+"
               icon="icons/user_agent_messages.png"
               id="org.netxms.ui.eclipse.agentmanager.popupActions.SendUserAgentNotification_Container"
               label="Send user support application notification"
               menubarPath="objectcontrol"
               style="push">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.netxms.ui.eclipse.agentmanager.actions.popup.object.ServiceRoot"
            objectClass="org.netxms.client.objects.ServiceRoot">
         <action
               class="org.netxms.ui.eclipse.agentmanager.actions.SendUserAgentNotification"
               enablesFor="+"
               icon="icons/user_agent_messages.png"
               id="org.netxms.ui.eclipse.agentmanager.popupActions.SendUserAgentNotification_ServiceRoot"
               label="Send user support application notification"
               menubarPath="objectcontrol"
               style="push">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.netxms.ui.eclipse.agentmanager.actions.popup.object.Rack"
            objectClass="org.netxms.client.objects.Rack">
         <action
               class="org.netxms.ui.eclipse.agentmanager.actions.SendUserAgentNotification"
               enablesFor="+"
               icon="icons/user_agent_messages.png"
               id="org.netxms.ui.eclipse.agentmanager.popupActions.SendUserAgentNotification_Rack"
               label="Send user support application notification"
               menubarPath="objectcontrol"
               style="push">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.netxms.ui.eclipse.agentmanager.actions.popup.object.Cluster"
            objectClass="org.netxms.client.objects.Cluster">
         <action
               class="org.netxms.ui.eclipse.agentmanager.actions.SendUserAgentNotification"
               enablesFor="+"
               icon="icons/user_agent_messages.png"
               id="org.netxms.ui.eclipse.agentmanager.popupActions.SendUserAgentNotification_Cluster"
               label="Send user support application notification"
               menubarPath="objectcontrol"
               style="push">
         </action>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="org.netxms.ui.eclipse.action_set.agent_management"
            label="%actionSet.AgentManagement"
            visible="true">
         <action
               class="org.netxms.ui.eclipse.agentmanager.actions.OpenAgentConfigManager"
               definitionId="org.netxms.ui.eclipse.agentmanager.commands.open_agent_configuration_manager"
               icon="icons/config_manager.gif"
               id="org.netxms.ui.eclipse.agentmanager.actions.open_agent_config_manager"
               label="%action.openAgentConfigManager"
               menubarPath="config/additions"
               toolbarPath="config/additions"
               tooltip="%actionDescription.openAgentConfigManager">
         </action>
         <action
               class="org.netxms.ui.eclipse.agentmanager.actions.OpenPackageManager"
               definitionId="org.netxms.ui.eclipse.agentmanager.commands.open_package_manager"
               icon="icons/packages.png"
               id="org.netxms.ui.eclipse.agentmanager.actions.open_package_manager"
               label="%action.openPackageManager"
               menubarPath="config/additions"
               toolbarPath="config/additions"
               tooltip="%actionDescription.openPackageManager">
         </action>
         <action
               class="org.netxms.ui.eclipse.agentmanager.actions.OpenTunnelManager"
               definitionId="org.netxms.ui.eclipse.agentmanager.commands.open_tunnel_manager"
               icon="icons/tunnel_manager.png"
               id="org.netxms.ui.eclipse.agentmanager.actions.open_tunnel_manager"
               label="%action.openTunnelManager"
               menubarPath="config/additions"
               style="push"
               toolbarPath="config/additions"
               tooltip="%actionDescription.openTunnelManager">
         </action>
         <action
               class="org.netxms.ui.eclipse.agentmanager.actions.OpenUserAgentNotificationView"
               definitionId="org.netxms.ui.eclipse.agentmanager.commands.open_user_agent_messages"
               icon="icons/user_agent_messages.png"
               id="org.netxms.ui.eclipse.agentmanager.actions.open_user_agent_messages"
               label="User Support Application Notifications"
               menubarPath="config/additions"
               style="push"
               tooltip="Open User Support Application Notifications">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.netxms.ui.eclipse.console.category.views"
            description="%actionDescription.openPackageManager"
            id="org.netxms.ui.eclipse.agentmanager.commands.open_package_manager"
            name="%action.openPackageManager">
      </command>
      <command
            categoryId="org.netxms.ui.eclipse.console.category.views"
            description="Open agent configuration manager"
            id="org.netxms.ui.eclipse.agentmanager.commands.open_agent_configuration_manager"
            name="Agent configuration manager">
      </command>
      <command
            id="org.netxms.ui.eclipse.agentmanager.commands.save_screenshot"
            name="Save screenshot">
      </command>
      <command
            id="org.netxms.ui.eclipse.agentmanager.commands.copy_screenshot"
            name="Copy screenshot">
      </command>
      <command
            categoryId="org.netxms.ui.eclipse.console.category.views"
            description="%actionDescription.openTunnelManager"
            id="org.netxms.ui.eclipse.agentmanager.commands.open_tunnel_manager"
            name="%action.openTunnelManager">
      </command>
      <command
            description="Toggle Filter"
            id="org.netxms.ui.eclipse.agentmanager.commands.show_filter"
            name="Toggle Filter">
      </command>
      <command
            categoryId="org.netxms.ui.eclipse.console.category.views"
            description="Open User Support Application Notifications"
            id="org.netxms.ui.eclipse.agentmanager.commands.open_user_agent_messages"
            name="User Support Application Notifications">
      </command>
      <command
            description="Toggle user session filter"
            id="org.netxms.ui.eclipse.datacollection.commands.show_user_session_filter"
            name="Toggle user session filter">
         <defaultHandler
               class="org.netxms.ui.eclipse.agentmanager.actions.ToggleUserSessionFilterHandler">
            <parameter
                  name="default"
                  value="default">
            </parameter>
         </defaultHandler>
         <state
               class="org.eclipse.jface.commands.ToggleState"
               id="org.netxms.ui.eclipse.datacollection.commands.show_user_session_filter.state">
         </state>
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.contexts">
      <context
            id="org.netxms.ui.eclipse.agentmanager.context.ScreenshotView"
            name="Screenshot view"
            parentId="org.eclipse.ui.contexts.window">
      </context>
      <context
            id="org.netxms.ui.eclipse.agentmanager.context.TunnelManager"
            name="Tunnel Manager"
            parentId="org.eclipse.ui.contexts.window">
      </context>
      <context
            id="org.netxms.ui.eclipse.agentmanager.context.ConfigEditor"
            name="Agent Config Editor"
            parentId="org.eclipse.ui.contexts.window">
      </context>
      <context
            id="org.netxms.ui.eclipse.agentmanager.context.UserAgentNotificationView"
            name="User Support Application Notifications"
            parentId="org.eclipse.ui.contexts.window">
      </context>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.netxms.ui.eclipse.agentmanager.commands.save_screenshot"
            contextId="org.netxms.ui.eclipse.agentmanager.context.ScreenshotView"
            schemeId="org.netxms.ui.eclipse.defaultKeyBinding"
            sequence="M1+S">
      </key>
      <key
            commandId="org.netxms.ui.eclipse.agentmanager.commands.copy_screenshot"
            contextId="org.netxms.ui.eclipse.agentmanager.context.ScreenshotView"
            schemeId="org.netxms.ui.eclipse.defaultKeyBinding"
            sequence="M1+C">
      </key>
      <key
            commandId="org.netxms.ui.eclipse.agentmanager.commands.show_filter"
            contextId="org.netxms.ui.eclipse.agentmanager.context.TunnelManager"
            schemeId="org.netxms.ui.eclipse.defaultKeyBinding"
            sequence="M1+F2">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.activities">
      <activity
            id="org.netxms.ui.eclipse.agentmanager.activities.SendUserAgentNotification"
            name="User Support Application Notification">
         <enabledWhen>
            <with
                  variable="org.netxms.access.UserAgentNotifications">
               <equals
                     value="true">
               </equals>
            </with>
         </enabledWhen>
      </activity>
      <activityPatternBinding
            activityId="org.netxms.ui.eclipse.agentmanager.activities.SendUserAgentNotification"
            isEqualityPattern="true"
            pattern="org.netxms.ui.eclipse.agentmanager/org.netxms.ui.eclipse.agentmanager.actions.open_user_agent_messages">
      </activityPatternBinding>
      <activityPatternBinding
            activityId="org.netxms.ui.eclipse.agentmanager.activities.SendUserAgentNotification"
            isEqualityPattern="true"
            pattern="org.netxms.ui.eclipse.agentmanager/horg.netxms.ui.eclipse.agentmanager.popupActions.SendUserAgentNotification">
      </activityPatternBinding>
   </extension>
   <extension
         point="org.netxms.ui.eclipse.objectview.tabs">
      <tab
            class="org.netxms.ui.eclipse.agentmanager.objecttabs.UserSessionsTab"
            icon="icons/user.png"
            id="org.netxms.ui.eclipse.agentmanager.objecttabs.UserSessionsTab"
            name="User Sessions"
            order="70">
      </tab>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.netxms.ui.eclipse.objectview.view.tabbed_object_view?after=additions">
         <command
               commandId="org.netxms.ui.eclipse.datacollection.commands.show_user_session_filter"
               label="Show user session filter"
               style="toggle"
               tooltip="Show user session filter">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="org.netxms.ui.eclipse.objectview.ActiveTab">
                  <instanceof
                        value="org.netxms.ui.eclipse.agentmanager.objecttabs.UserSessionsTab">
                  </instanceof>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>

</plugin>
